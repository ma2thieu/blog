<head>

	<meta charset="utf-8">
	<title></title>
	<meta name="description" content="">
	<meta name="keywords" content="" >
	<meta name="author" content="weebox">
	<!--[if lt IE 9]>
        <script src="{{ asset('js/html5shim/html5.js') }}" type="text/javascript"></script>
	<![endif]-->

	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    {% block stylesheets %}
        <link href="{{ asset('css/skeleton/base.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('css/skeleton/skeleton.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('css/skeleton/layout.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('css/bootstrap/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('css/jquery-ui/smoothness/jquery-ui-1.8.16.custom.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('css/screen.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}

    {% block my_css %}
    {% endblock %}

	<link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}">

</head>
<body>

    <div class="container">
    {% block container %}

        <div id="language" class="sixteen columns">
        {% block language %}
        {% endblock %}
        </div>

        <div id="header" class="sixteen columns">
        {% block header %}
        {% endblock %}
        </div>

        <div id="content" class="sixteen columns">
        {% block content %}
        {% endblock %}
        </div>

        <div id="footer" class="sixteen columns">
        {% block footer %}
        {% endblock %}
        </div>

    {% endblock %}
    </div>

	{% block javascripts %}
	    <script src="{{ asset('js/jquery/jquery-1.7.1.min.js') }}" type="text/javascript"></script>
	    <script src="{{ asset('js/jquery-ui/jquery-ui-1.8.16.custom.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/skeleton/tabs.js') }}" type="text/javascript"></script>
		<script src="{{ asset('js/bootstrap/bootstrap.min.js') }}" type="text/javascript"></script>
    {% endblock %}


    {% block my_js %}
    {% endblock %}

</body>
